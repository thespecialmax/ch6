var FixedContainer=function(el,opt){var $window=$(window);var $document=$(document);var $el=$(el);var $obj=this;var options=$.extend({minWindowWidth:991,offsetTop:null,positionEnd:null},opt);var offsetTop=options.offsetTop;var setWidth=false;this.init=function(){if($window.width()>options.minWindowWidth){if(!$el.hasClass("fixedcontainer")){$el.addClass("fixedcontainer");}
